{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:testing": "vite --config vite.config.testing.js --mode testing",
        "test:e2e": "npx playwright test",
        "test:unit": "vitest run --coverage --coverage.reporter=text --config ./vitest.config.ts",
        "lint": "eslint resources/js --ext .js,.ts,.vue --ignore-path .gitignore --fix --config ./.eslintrc.cjs",
        "format": "prettier --write resources/js/"
    },
    "devDependencies": {
        "@pinia/testing": "^0.1.3",
        "@playwright/test": "^1.57.0",
        "@rushstack/eslint-patch": "^1.10.4",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/vite": "^4.0.0",
        "@tsconfig/node20": "^20.1.4",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vitest/coverage-v8": "^2.1.9",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.5.1",
        "axios": "^1.7.2",
        "concurrently": "^9.0.1",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.27.0",
        "flush-promises": "^1.0.2",
        "jsdom": "^24.1.1",
        "laravel-vite-plugin": "^1.0.4",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "tailwindcss": "^4.0.0",
        "typescript": "~5.5.3",
        "vite": "^5.3.5",
        "vitest": "^2.0.5",
        "vue": "^3.5.3",
        "vue-eslint-parser": "^10.2.0",
        "vue-tsc": "^2.0.24"
    },
    "dependencies": {
        "laravel-echo": "^1.16.1",
        "pinia": "^2.2.0",
        "pusher-js": "^8.4.0-rc2",
        "vue-router": "^4.4.0"
